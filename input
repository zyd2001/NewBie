array a = [1, 2, 3, [4, 5]];
println(print_array(a) + "item(s) in the array");

int print_array(array a)
{
    string type;
    int count;
    foreach (x in a) // also for (x in a)
    {
        type = getType(x);
        if (type != "array")
        {
            println(type + " " + x);
        }
        else
        {
            println("array:");
            print_array(x);
            println("array end");
        }
        count++;
    }
    return count;
}